{
  "name": "andela-teams",
  "version": "0.0.1",
  "description": "This is the web UI for Andela Teams",
  "main": "index.js",
  "engines": {
    "node": "9.7.1",
    "npm": "5.6.0",
    "yarn": "1.5.1"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.js --progress",
    "build-and-start": "npm run build && npm start",
    "build-and-start:dev": "npm run build && npm run start:dev",
    "eslint": "./node_modules/.bin/eslint src",
    "eslint-fix": "./node_modules/.bin/eslint --fix src",
    "heroku-postbuild": "npm run build",
    "start": "node --require babel-register server.js",
    "start:dev": "NODE_ENV=development nodemon --require babel-register server.js",
    "test": ""
  },
  "pre-commit": [
    "eslint"
  ],
  "repository": "https://github.com/andela-stuff/andela-teams",
  "author": "Franklin Chieze <chieze.franklin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.10",
    "dotenv-webpack": "^1.5.5",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "immutable": "^3.8.2",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "node-sass": "^4.8.1",
    "nodemon": "^1.17.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.4",
    "react-blur-admin": "^0.11.1",
    "react-dom": "^16.2.0",
    "react-flex-proto": "^1.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "pre-commit": "^1.2.2"
  }
}
